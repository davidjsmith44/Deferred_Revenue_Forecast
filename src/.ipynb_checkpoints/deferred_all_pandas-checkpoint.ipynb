    "    \n",
    "    #groupby the columns passed in the function call then remove the subscription term column\n",
    "    gb = data2process.groupby(gblist, as_index=False).sum()\n",
    "    gb.drop(labels='Subscription Term', axis=1, inplace = True)\n",
    "    return gb"
